package org.zerock.util;

import lombok.Getter;
import lombok.Setter;
import lombok.ToString;

@ToString
@Setter
@Getter
public class Criteria {
	private int pageNum;
	private int amount;

	private String type;
	private String keyword;

	public Criteria() {
		this(1,10);
	}
	
	Criteria		this.pageNum = pageNum;
		this.amount = amount;
	}
	
	

}
